<!--幸运者详情页-->
<!-- 
    header:用户头像
    user_name:名字
    title:标题（第几期幸运者）
    info:用户信息
    dream_info:梦想详情
    textarea：梦想信息（文本内容）
 -->
<view class='lucky_user'>
    <view class='header'>
        <image src='{{targetInfo.headicon}}'></image>
    </view>
    <view class='user_name'>{{targetInfo.nickname}}</view>
    <view class='title'>小梦想互助{{targetInfo.pid}}期幸运者</view>
    <view class='info'>
        <view class='icon_flag'></view>
        <text>福彩{{targetInfo.expect}}期</text>
    </view>
    <view class='dream_info'>
        <view class='dream_title'>梦 想</view>
        <view class='textarea'>{{targetInfo.title}}</view>
        <view class='dream_title'>梦想详情</view>
        <view class='textarea'>{{targetInfo.content}}
        </view>
        <view class='dream_title'>相关照片</view>
        <view class='textarea'>暂无照片</view>
        <!--image src='http://tdream.antit.top/image/pasion.png'></image>
        <image src='http://tdream.antit.top/image/pasion.png'></image-->
    </view>
</view>
