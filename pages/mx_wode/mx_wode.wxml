<!-- <view class='container'> -->
<view class='mx_container'>
 <view class='page-header'>
   <view class='col box_goumai'>
      <view class='col-4'>
         <text class='bloc'><text class='color_red'>{{owner.totalJoin}}</text><text style='color:white'>个</text></text>
          <text class='bloc' style='color:#ffffff'>参与中的梦想</text>
      </view>
      <view class='col-4'>
         <text class='bloc'><text class='color_red'>{{owner.totalReward.value}}</text><text style='color:white'>{{owner.totalReward.unit}}</text></text>
          <text class='bloc' style='color:#ffffff'>已获得互助总金额</text>
      </view>
      <view class='col-4'>
         <text class='bloc'><text class='color_red'>{{owner.dayBuy}}</text><text style='color:white'>份</text></text>
          <text class='bloc' style='color:#ffffff'>今日参与份数</text>
      </view>
    </view>
  </view>
 <view class='page-body'>
  <view class='box_huzhu row'>
  <!-- 梦想池 -->
    <view bindtap='myPool' class=' col box_list'>
      <text class='col-11'>我参与互助的梦想池</text>
      <text class='col-1'>></text>
    </view>
     <!-- 梦想池 -->
     <navigator navigator="true" url="../mx_phone/mx_phone"><view class=' col box_list'>
     <text class='col-11'>手机号管理</text>
      <text class='col-1'>></text>
    </view></navigator>
     <!-- 梦想池 -->
    <navigator navigator="true" url="../mx_shenfenzheng/mx_shenfengzheng">
    <view class=' col box_list'>
      <text class='col-11'>实名认证</text>
      <text class='col-1'>></text>
    </view>
    </navigator>
     <!-- 梦想池 -->
    <!--view class=' col box_list'>
      <text class='col-11'>池主申请</text>
      <text class='col-1'>>></text>
    </view-->
     <!-- 梦想池 --> 
      <navigator navigator="true" url="../mx_wenti/mx_wenti">
    <view class=' col box_list'>
      <text class='col-11'>常见问题</text>
      <text class='col-1'>></text>
    </view>
    </navigator>
     <!-- 梦想池 -->
    <view bindtap='onCommunicate'  class=' col box_list'>
      <text class='col-11'>联系客服</text>
      <text class='col-1'>></text>
    </view>
    
      <button class='col box_list' style='border:none;float:left;width:100%;margin-left:-1px;' open-type="feedback" ><text style='float:left;text-align:left;width:95%' open-type="feedback">用户反馈</text><text style='float:right;text-align:right;'>></text></button>
      
    
  </view>
  
 </view>

 <!-- 底部导航栏 -->

 <view class="weui-footer_fixed-bottom m_footer row">
    <view class='col'>
      <view class='col-4'>
      <!-- sprite_canyu_act -->
      <navigator redirect="true" url="../index/index">
      <view class='sprite_xiaomengxiang'></view>
      <text class='text_footer'>参与互动</text>
       </navigator>
    </view>
    <view class='col-4'>
      <!-- sprite_xiaomengxiang_act -->
      <navigator redirect="true" url="../mx_naicha/mx_naicha">
      <view class='sprite_canyu'></view>
      <text class='text_footer'>小梦想</text>
       </navigator>
    </view>
    <view class='col-4 m_active'>
      <!-- sprite_wode_act -->
      <view class='sprite_wode_act'></view>
      <text class='text_footer'>个人中心</text>
    </view>
    </view>
  </view>


