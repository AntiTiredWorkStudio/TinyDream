/**app.wxss**/
@import "common/lib/weui.wxss";
@import "common/lib/iconfont.wxss";
@import "common/lib/widget/weui-button/weui-button.wxss";
page{
 background:#f1f4f7;
}
.container {
  height: 100%;
  display: block;
  width: 100%;
 background:#f1f4f7;
} 

.width_25{width: 25%}
.width_30{width: 30%}
.color_red{color: red;font-size: 25px}
.color_white{color: white}
.bloc{display: block;font-size: 15px}
.bloc1{display: block;font-size: 14px}
.page-head{margin: 15px;display: flex;flex-direction: column;border-radius: 10px;background: white;}
.m_cheng{background:#f1f4f7;height:10px}
.m_footer{background: white;border-top: thin solid #ebebeb}
.m_footer .footer_logo{width:50px;height:50px;text-align:center}
.m_active{color: #5ECD98}
.mt_10{margin: 10px}
.mt_20{margin-top: 20px}
/* 梦想池box */
.menu{padding-top: 10px;margin: 5px}
.memu_act{border-bottom: solid medium #04bbff}
.menu_unact{border-bottom:solid medium #bfbfbf;}
.box_huzhu .box_chizi{width: 40%;background: #5ecd98;border-radius:5px;opacity: 0.8;text-align: center;color: white;padding: 5px 5px 5px 5px;font-size: 30rpx;position: absolute;left: 20px}
/* 未中奖 */
.no_reward{position: relative}
.no_reward .pic{position: absolute;background-image: url(http://cloud.antit.top/no_reward.png);z-index: 111;background-repeat:no-repeat; width: 131px;height: 62px;left: 50%;margin-left: -40px}
.reward .pic{position: absolute;background-image: url(http://cloud.antit.top/reward.png);z-index: 111;width: 158px;height: 128px;left: 50%;margin-left: -79px;margin-top:-30px}
.cent{text-align: center}
.col_jindu{position: relative}
.col_baifenbi{color: red;position: absolute;left: 45%;top:-5px}
/* 栅格化 */
.row {
  display: block;
  margin: 0px;
}

.col {
  display: flex;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  font-size: 17px;
}

.col>.col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9,
.col-10, .col-11, .col-12 {
  overflow: hidden;
}

